{
  "name": "joint-fpl-lib",
  "version": "1.1.0",
  "description": "TypeScript library for building Fantasy Premier League Draft dashboards with Observable Framework",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./utils/colours": {
