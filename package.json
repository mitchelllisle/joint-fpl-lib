      "import": "./dist/utils/noDataTextMark.js",
      "types": "./dist/utils/noDataTextMark.d.ts"
    },
    "./charts/bumpChart": {
      "import": "./dist/charts/bumpChart.js"
    },
    "./charts/pointsPerWeek": {
      "import": "./dist/charts/pointsPerWeek.js"
    },
    "./charts/pointsBarChart": {
      "import": "./dist/charts/pointsBarChart.js"
    },
    "./charts/giniIndexChart": {
      "import": "./dist/charts/giniIndexChart.js"
    },
    "./charts/benchPoints": {
      "import": "./dist/charts/benchPoints.js"
    },
    "./charts/positionBreakdown": {
      "import": "./dist/charts/positionBreakdown.js"
    },
    "./charts/consistencyBullet": {
      "import": "./dist/charts/consistencyBullet.js"
    },
    "./charts/formChart": {
      "import": "./dist/charts/formChart.js"
    },
    "./charts/bonusPoints": {
      "import": "./dist/charts/bonusPoints.js"
    },
    "./charts/bumpChart.js": {
      "import": "./dist/charts/bumpChart.js"
    },
    "./charts/pointsPerWeek.js": {
      "import": "./dist/charts/pointsPerWeek.js"
    },
    "./charts/pointsBarChart.js": {
      "import": "./dist/charts/pointsBarChart.js"
    },
    "./charts/giniIndexChart.js": {
      "import": "./dist/charts/giniIndexChart.js"
    },
    "./charts/benchPoints.js": {
      "import": "./dist/charts/benchPoints.js"
    },
    "./charts/positionBreakdown.js": {
      "import": "./dist/charts/positionBreakdown.js"
    },
    "./charts/consistencyBullet.js": {
      "import": "./dist/charts/consistencyBullet.js"
    },
    "./charts/formChart.js": {
      "import": "./dist/charts/formChart.js"
    },
    "./charts/bonusPoints.js": {
      "import": "./dist/charts/bonusPoints.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts"
  },
  "keywords": [
    "fantasy-premier-league",
    "fpl",
    "draft",
    "observable",
    "charts",
    "visualization"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mitchelllisle/joint-fpl-lib.git"
  },
  "homepage": "https://github.com/mitchelllisle/joint-fpl-lib#readme",
  "devDependencies": {
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "@vitest/coverage-v8": "^1.1.0",
